title = {{Finite element study of vortex-induced cross-flow and in-line oscillations of a circular cylinder at low Reynolds numbers}},
url = {http://doi.wiley.com/10.1002/(SICI)1097-0363(19991215)31:7%3C1087::AID-FLD911%3E3.0.CO;2-C},
