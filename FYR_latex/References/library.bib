isbn = {1469-7645},
issn = {0022-1120},
journal = {Journal of fluid mechanics},
keywords = {4711,4732F,Alignement,Alignment,Cilindro circular,Circular cylinder,Configuraci{\'{o}}n geom{\'{e}}trica,Configuraci{\'{o}}n paralela,Configuration g{\'{e}}om{\'{e}}trique,Configuration parall{\`{e}}le,Cylindre circulaire,Desprendimiento vorticial,Digital simulation,D{\'{e}}tachement tourbillonnaire,Ecoulement tourbillonnaire,Finite element method,Geometrical configuration,Low Reynolds number,Modelling,Mod{\'{e}}lisation,M{\'{e}}thode {\'{e}}l{\'{e}}ment fini,Nombre Reynolds faible,N{\'{u}}mero Reynolds d{\'{e}}bil,Parallel configuration,Simulation num{\'{e}}rique,Vortex flow,Vortex shedding},
